{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "envalid": "^5.0.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.19"
  },
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=production node ./build/index.js",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "tslint": "tslint --project tsconfig.json",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "yarn prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "yarn prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "typeorm:cli": "ts-node-dev ./node_modules/typeorm/cli -f ./src/ormconfig.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn  prettier:write"
    }
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/faker": "^4.1.6",
    "@types/helmet": "^0.0.44",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.4",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node-dev": "^1.0.0-pre.43",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  }
}
